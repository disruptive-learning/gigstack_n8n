{
    "name": "n8n-nodes-gigstack",
    "version": "1.1.0",
    "description": "n8n node for Gigstack API integration",
    "keywords": [
        "n8n-community-node-package",
        "n8n-node-gigstack",
        "gigstack"
    ],
    "license": "MIT",
    "homepage": "https://gigstack.com",
    "author": {
        "name": "Your Name",
        "email": "your@email.com"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/yourusername/n8n-nodes-gigstack.git"
    },
    "main": "index.js",
    "scripts": {
        "build": "tsc && gulp build:icons",
        "dev": "tsc --watch",
        "format": "prettier nodes credentials --write",
        "lint": "eslint nodes credentials package.json",
        "lintfix": "eslint nodes credentials package.json --fix",
        "prepublishOnly": "npm run build && npm run lint -c .eslintrc.prepublish.js nodes credentials package.json"
    },
    "files": [
        "dist"
    ],
    "n8n": {
        "n8nNodesApiVersion": 1,
        "credentials": [
            "dist/credentials/GigstackApi.credentials.js"
        ],
        "nodes": [
            "dist/nodes/Gigstack.node.js"
        ]
    },
    "devDependencies": {
        "@types/express": "^4.17.17",
        "@types/request-promise-native": "^1.0.18",
        "@typescript-eslint/parser": "^5.0.1",
        "eslint-plugin-n8n-nodes-base": "^1.0.0",
        "gulp": "^4.0.2",
        "n8n-core": "^0.125.0",
        "n8n-workflow": "^0.107.0",
        "prettier": "^2.3.2",
        "typescript": "~4.8.4"
    },
    "dependencies": {
        "axios": "^1.3.4"
    }
}